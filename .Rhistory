n=1000
vrijeme=seq(0,n,by=1)
p0=10
mi=0.1
sigma=0.2
logpovrat=rep(0,n+1)
logcijena=rep(0,n+1)
cijena=rep(0,n+1)
ocekivanje=rep(0,n+1)
logcijena[1]=log(p0)
cijena[1]=p0
ocekivanje[1]=p0
for(i in 2:n+1)
{logpovrat[i]=mi/365+(sigma/sqrt(365))*rnorm(1,0,1)
logcijena[i]=log(p0)+sum(logpovrat)
cijena[i]=p0*exp(sum(logpovrat))
ocekivanje[i]=p0*exp(i*mi/365+i*sigma^2/(2*365))
}
plot(vrijeme,logpovrat,type="l",col="red",xlab="vrijeme",ylab="logpovrati")
qqnorm(logpovrat); qqline(logpovrat)
